{
    "version": "0.1",
    "provider": {
        "name": "kevel",
        "schema": {
            "attributes": [
                {
                    "name": "api_base_url",
                    "string": {
                        "description": "The base URL of the Kevel API. This can also be set via the KEVEL_API_BASE_URL environment variable.",
                        "optional_required": "optional"
                    }
                },
                {
                    "name": "api_key",
                    "string": {
                        "description": "Your Kevel API Key. This can also be set via the KEVEL_API_KEY environment variable.",
                        "optional_required": "optional",
                        "sensitive": true
                    }
                }
            ]
        }
    },
    "resources": [
        {
            "name": "site",
            "schema": {
                "attributes": [
                    {
                        "name": "id",
                        "int64": {
                            "description": "Numeric identifier of the site",
                            "computed_optional_required": "computed"
                        }
                    },
                    {
                        "name": "title",
                        "string": {
                            "description": "Title of the site",
                            "computed_optional_required": "required"
                        }
                    },
                    {
                        "name": "url",
                        "string": {
                            "description": "URL of the site",
                            "computed_optional_required": "required"
                        }
                    }
                ]
            }
        }
    ],
    "datasources": []
}